<header>
  <h1><a href="/">Members Only</a></h1>
  <ul>
    <% if (!user) {%>
    <li><a href="/log-in">Log In</a></li>
    <li><a href="/sign-up">Sign Up</a></li>
    <% } else if (user) {%>
    <li><a href="/log-out">Log Out</a></li>
    <% if (!user.member) {%>
    <li><a href="/join-member">Become a member</a></li>
    <% } if (!user.admin) {%>
    <li><a href="/become-admin">Become an admin</a></li>
    <% } } %>
  </ul>
</header>
